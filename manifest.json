{
  "name": "Linkedin Feed Sucks",
  "description": "Hide Linkedin BS Stream",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
        "matches": [
          "https://www.linkedin.com/*"
        ]
    }
  ],
  "background": {
    "service_worker": "scripts/service-worker.js"
  },
  "permissions": [
    "tabs"
  ]
}